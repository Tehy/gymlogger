<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gymlogger">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ProgramsActivity" android:title="ProgramsActivity"></activity>
        <activity android:name=".TrainingsActivity" android:title="TrainingsActivity"></activity>
        <activity android:name=".exercisesActivity" android:title="exercisesActivity"></activity>
        <activity android:name=".WorkoutsActivity" android:title="WorkoutsActivity"></activity>
        <activity android:name=".WorkoutStartActivity" android:title="WorkoutStartActivity"></activity>
        <activity android:name=".WorkoutViewExerciseActivity" android:title="WorkoutStartExerciseActivity"></activity>
        <activity android:name=".WorkoutsViewWorkoutWorksetsActivity" android:title="WorkoutsViewWorkoutWorksetsActivity"></activity>
        <activity android:name=".TimerActivity" android:title="TimerActivity"></activity>

    </application>


</manifest>